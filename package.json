    "@unocss/eslint-config": "catalog:build",
    "@unocss/nuxt": "catalog:build",
    "@vite-pwa/nuxt": "catalog:build",
    "@vitest/coverage-v8": "catalog:testing",
    "@vitest/ui": "catalog:testing",
    "@vue/test-utils": "catalog:testing",
    "@vueuse/core": "catalog:frontend",
    "@vueuse/nuxt": "catalog:build",
    "dayjs": "catalog:frontend",
    "dayjs-nuxt": "catalog:build",
    "echarts": "catalog:frontend",
    "eslint": "catalog:dev",
    "eslint-plugin-format": "catalog:dev",
    "happy-dom": "catalog:testing",
    "lint-staged": "catalog:dev",
    "nuxt": "catalog:build",
    "nuxt-echarts": "catalog:build",
    "nuxt-qrcode": "catalog:build",
    "nuxt-swiper": "catalog:build",
    "nuxt-typed-router": "catalog:build",
    "pinia": "catalog:frontend",
    "playwright-core": "catalog:testing",
    "simple-git-hooks": "catalog:dev",
    "std-env": "catalog:testing",
    "typescript": "catalog:dev",
    "unocss": "catalog:build",
    "unplugin-vue-router": "catalog:build",
    "vitest": "catalog:testing",
    "vue": "catalog:frontend",
    "vue-echarts": "catalog:frontend",
    "vue-tsc": "catalog:dev"
  },
  "simple-git-hooks": {
    "pre-commit": "bun run lint-staged"
  },
  "lint-staged": {
    "*": "bun run eslint --fix"
  },
  "workspaces": {
    "catalogs": {
      "build": {
        "@nuxt/eslint": "^1.12.1",
