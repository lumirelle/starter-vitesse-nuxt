{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.23.0",
  "scripts": {
    "build": "nuxt build",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxt dev -o",
    "dev": "nuxt dev -o",
    "generate": "nuxt generate",
    "prepare": "nuxt prepare && simple-git-hooks",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "start:generate": "na exec serve .output/public",
    "lint": "eslint .",
    "typecheck": "nuxt typecheck",
    "test": "vitest",
    "test:unit": "vitest --project unit",
    "pretest:e2e": "nlx playwright-core install --with-deps",
    "test:e2e": "vitest --project e2e",
    "test:nuxt": "vitest --project nuxt"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:dev",
    "@antfu/ni": "catalog:dev",
    "@antfu/utils": "catalog:frontend",
    "@iconify-json/carbon": "catalog:icons",
    "@iconify-json/twemoji": "catalog:icons",
    "@nuxt/eslint": "catalog:build",
    "@nuxt/fonts": "catalog:build",
    "@nuxt/image": "catalog:build",
    "@nuxt/test-utils": "catalog:build",
    "@nuxtjs/color-mode": "catalog:build",
    "@nuxtjs/device": "catalog:build",
    "@nuxtjs/i18n": "catalog:build",
    "@nuxtjs/seo": "catalog:build",
    "@pinia/nuxt": "catalog:build",
    "@unocss/eslint-config": "catalog:build",
    "@unocss/nuxt": "catalog:build",
    "@vite-pwa/nuxt": "catalog:build",
    "@vitest/coverage-v8": "catalog:testing",
    "@vitest/ui": "catalog:testing",
    "@vue/test-utils": "catalog:testing",
    "@vueuse/core": "catalog:frontend",
    "@vueuse/nuxt": "catalog:build",
    "czg": "catalog:dev",
    "dayjs": "catalog:frontend",
    "dayjs-nuxt": "catalog:build",
    "echarts": "catalog:frontend",
    "eslint": "catalog:dev",
    "eslint-plugin-format": "catalog:dev",
    "happy-dom": "catalog:testing",
    "lint-staged": "catalog:dev",
    "nuxt": "catalog:build",
    "nuxt-echarts": "catalog:build",
    "nuxt-qrcode": "catalog:build",
    "nuxt-svgo": "catalog:build",
    "nuxt-swiper": "catalog:build",
    "nuxt-typed-router": "catalog:build",
    "pinia": "catalog:frontend",
    "playwright-core": "catalog:testing",
    "simple-git-hooks": "catalog:dev",
    "taze": "catalog:dev",
    "typescript": "catalog:dev",
    "unocss": "catalog:build",
    "unplugin-vue-router": "catalog:build",
    "vitest": "catalog:testing",
    "vue": "catalog:frontend",
    "vue-echarts": "catalog:frontend",
    "vue-tsc": "catalog:dev"
  },
  "simple-git-hooks": {
    "pre-commit": "na exec lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
