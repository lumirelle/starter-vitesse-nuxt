{
  "type": "module",
  "private": true,
  "engines": {
    "node": "^20.19 || >=22.12"
  },
  "scripts": {
    "prepare": "bun --parallel 'nuxt prepare' simple-git-hooks",
    "dev": "bun nuxt dev -o",
    "dev:pwa": "VITE_PLUGIN_PWA=true bun nuxt dev -o",
    "build": "bun nuxt build",
    "generate": "bun nuxt generate",
    "start": "bun .output/server/index.mjs",
    "start:generate": "bunx serve .output/public",
    "format": "bunx oxfmt",
    "lint": "bun --parallel oxlint eslint",
    "lint:fix": "bun --sequential 'oxlint --fix' 'eslint --fix'",
    "check:type": "bun nuxt typecheck",
    "check:usage": "echo 'WIP...",
    "check": "bun run --parallel lint check:type check:usage",
    "pretest": "bun playwright-core install --with-deps",
    "test": "bunx vitest",
    "test:unit": "bun run test --project unit",
    "test:e2e": "bun run test --project e2e",
    "test:nuxt": "bun run test --project nuxt"
  },
  "workspaces": {
    "catalogs": {
      "frontend": {
        "@antfu/utils": "^9.3.0",
        "@takumi-rs/core": "^0.69.3",
        "@vueuse/core": "^14.2.1",
        "dayjs": "^1.11.19",
        "echarts": "^6.0.0",
        "pinia": "^3.0.4",
        "vue": "^3.5.29",
        "vue-echarts": "^8.0.1"
      },
      "icons": {
        "@iconify-json/carbon": "^1.2.18",
        "@iconify-json/twemoji": "^1.2.5",
        "@iconify/utils": "^3.1.0"
      },
      "build": {
        "@nuxt/eslint": "^1.15.2",
        "@nuxt/fonts": "^0.14.0",
        "@nuxt/hints": "^1.0.0-alpha.9",
        "@nuxt/image": "npm:@nuxt/image-nightly@2.0.1-20251225-125234-23b8cef",
        "@nuxt/test-utils": "^4.0.0",
        "@nuxtjs/color-mode": "^4.0.0",
        "@nuxtjs/device": "^4.0.0",
        "@nuxtjs/i18n": "^10.2.3",
        "@nuxtjs/seo": "^3.4.0",
        "@pinia/nuxt": "^0.11.3",
        "@unocss/eslint-config": "^66.6.2",
        "@unocss/nuxt": "^66.6.2",
        "@vite-pwa/nuxt": "^1.1.1",
        "@vueuse/nuxt": "^14.2.1",
        "dayjs-nuxt": "^2.1.11",
        "nuxt": "^4.3.1",
        "nuxt-echarts": "^1.0.1",
        "nuxt-qrcode": "^0.4.8",
        "nuxt-swiper": "^2.0.1",
        "nuxt-typed-router": "^4.0.7",
        "unocss": "^66.6.2",
        "unplugin-vue-router": "^0.19.2"
      },
      "dev": {
        "simple-git-hooks": "git+https://github.com/toplenboren/simple-git-hooks.git#7625657",
        "typescript": "^5.9.3",
        "vue-tsc": "^3.2.5"
      },
      "check": {
        "@antfu/eslint-config": "^7.6.1",
        "eslint": "^9.39.3",
        "eslint-plugin-oxlint": "^1.50.0",
        "nano-staged": "^0.9.0",
        "oxfmt": "^0.35.0",
        "oxlint": "^1.50.0"
      },
      "test": {
        "@vitest/coverage-v8": "^4.0.18",
        "@vue/test-utils": "^2.4.6",
        "happy-dom": "^20.7.0",
        "playwright-core": "^1.58.2",
        "std-env": "^4.0.0-rc.1",
        "vitest": "^4.0.18"
      }
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:check",
    "@antfu/utils": "catalog:frontend",
    "@iconify-json/carbon": "catalog:icons",
    "@iconify-json/twemoji": "catalog:icons",
    "@iconify/utils": "catalog:icons",
    "@nuxt/eslint": "catalog:build",
    "@nuxt/fonts": "catalog:build",
    "@nuxt/hints": "catalog:build",
    "@nuxt/image": "catalog:build",
    "@nuxt/test-utils": "catalog:build",
    "@nuxtjs/color-mode": "catalog:build",
    "@nuxtjs/device": "catalog:build",
    "@nuxtjs/i18n": "catalog:build",
    "@nuxtjs/seo": "catalog:build",
    "@pinia/nuxt": "catalog:build",
    "@takumi-rs/core": "catalog:frontend",
    "@unocss/eslint-config": "catalog:build",
    "@unocss/nuxt": "catalog:build",
    "@vite-pwa/nuxt": "catalog:build",
    "@vitest/coverage-v8": "catalog:test",
    "@vue/test-utils": "catalog:test",
    "@vueuse/core": "catalog:frontend",
    "@vueuse/nuxt": "catalog:build",
    "dayjs": "catalog:frontend",
    "dayjs-nuxt": "catalog:build",
    "echarts": "catalog:frontend",
    "eslint": "catalog:check",
    "eslint-plugin-oxlint": "catalog:check",
    "happy-dom": "catalog:test",
    "nano-staged": "catalog:check",
    "nuxt": "catalog:build",
    "nuxt-echarts": "catalog:build",
    "nuxt-qrcode": "catalog:build",
    "nuxt-swiper": "catalog:build",
    "nuxt-typed-router": "catalog:build",
    "oxfmt": "catalog:check",
    "oxlint": "catalog:check",
    "pinia": "catalog:frontend",
    "playwright-core": "catalog:test",
    "simple-git-hooks": "catalog:dev",
    "std-env": "catalog:test",
    "typescript": "catalog:dev",
    "unocss": "catalog:build",
    "unplugin-vue-router": "catalog:build",
    "vitest": "catalog:test",
    "vue": "catalog:frontend",
    "vue-echarts": "catalog:frontend",
    "vue-tsc": "catalog:dev"
  },
  "nano-staged": {
    "*": [
      "bun oxlint",
      "bun eslint",
      "bunx oxfmt --no-error-on-unmatched-pattern"
    ]
  },
  "patchedDependencies": {
    "listhen@1.9.0": "patches/listhen@1.9.0.patch"
  },
  "overrides": {
    "nuxt-og-image": "npm:nuxt-og-image@v6.0.0-beta.41"
  },
  "simple-git-hooks": {
    "pre-commit": "bun nano-staged"
  }
}
