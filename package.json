{
  "type": "module",
  "private": true,
  "engines": {
    "node": "^20.19 || >=22.12"
  },
  "scripts": {
    "prepare": "bun run nuxt prepare && bun run simple-git-hooks",
    "dev": "bun run nuxt dev -o",
    "dev:pwa": "VITE_PLUGIN_PWA=true bun run nuxt dev -o",
    "build": "bun run nuxt build",
    "generate": "bun run nuxt generate",
    "start": "bun run .output/server/index.mjs",
    "start:generate": "bunx serve .output/public",
    "lint": "bun run eslint .",
    "typecheck": "bun run nuxt typecheck",
    "pretest": "bun run playwright-core install --with-deps",
    "test": "vitest",
    "test:unit": "npm run test --project unit",
    "test:e2e": "npm run test --project e2e",
    "test:nuxt": "npm run test --project nuxt"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:dev",
    "@antfu/utils": "catalog:frontend",
    "@iconify-json/carbon": "catalog:icons",
    "@iconify-json/twemoji": "catalog:icons",
    "@iconify/utils": "catalog:icons",
    "@nuxt/eslint": "catalog:build",
    "@nuxt/fonts": "catalog:build",
    "@nuxt/hints": "catalog:build",
    "@nuxt/image": "catalog:build",
    "@nuxt/test-utils": "catalog:build",
